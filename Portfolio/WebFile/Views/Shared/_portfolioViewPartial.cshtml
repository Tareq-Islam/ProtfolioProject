        <div class="portfolio-holder" id="Grid">
            <!--start portfolio-->
            @foreach (var item in ViewBag.Products)
            {
                if (item.IsActive != false)
                {
                    <div class="portfolio-post col-sm-6 col-md-4 mix @item.CategoryName">
                        <div class="thumb-post">
                            <div class="overlay">
                                <div class="overlay-inner">
                                    <div class="portfolio-infos">
                                        <span class="meta-category">@item.CategoryName</span>
                                        <h3 class="portfolio-title"><a href="#">@item.ProductName</a></h3>
                                    </div>
                                    <div class="portfolio-expand">
                                        <a class="fancybox" href="@item.FilePathOrLink" title="@item.ProductName">
                                            <i class="fa fa-expand"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            <img src="@item.FilePathOrLink" alt="@item.ProductName">
                        </div>
                    </div> <!-- /.col-md-4 -->
                }
            }

